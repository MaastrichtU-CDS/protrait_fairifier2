map:patient rr:predicateObjectMap [
        rr:predicateObjectMap [
        rr:predicate roo:P100262; #has_dysphagia 
        rr:objectMap [
            rr:parentTriplesMap map:dysphagia;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];        
        ];
    ];
     rr:predicateObjectMap [
